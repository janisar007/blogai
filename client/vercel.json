{
  "rewrites": [
    {
      "source": "/robots.txt",
      "destination": "https://blogai-six.vercel.app/robots.txt"
    },
    {
      "source": "/sitemap.xml",
      "destination": "https://blogai-six.vercel.app/sitemap.xml"
    },
    {
      "source": "/blog/:id",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)(googlebot|bingbot|slurp|duckduckbot|baiduspider|yandex|facebookexternalhit|twitterbot)"
        }
      ],
      "destination": "https://blogai-six.vercel.app/blog/:id"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
